+    public int getLeft(int row, int col, int cols, boolean firstRowLarge) {
+        final int cw = row == 0 && firstRowLarge ? mLargeCellWidth : mCellWidth;
+        final int extra = (getWidth() - cw * cols) / (cols + 1);
+        int left = col * cw + (col + 1) * extra;
